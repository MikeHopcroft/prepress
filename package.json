    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/command-line-usage": "^5.0.1",
    "@types/mkdirp": "^1.0.1",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.11.2",
    "@types/recursive-readdir": "^2.2.0",
    "@types/sinon": "^10.0.0",
    "@types/sinon-chai": "^3.2.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "fs-monkey": "^1.0.3",
    "gts": "^3.0.3",
    "memfs": "^3.2.2",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^10.0.0",
    "sinon-chai": "^3.6.0",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "command-line-usage": "^6.1.1",
    "minimist": "^1.2.5",
    "mkdirp": "^0.5.5",
    "recursive-readdir": "^2.2.2"
  }
}
